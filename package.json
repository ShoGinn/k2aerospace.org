{
  "name": "k2aerospace_website",
  "version": "0.0.32",
  "description": "K2 Aersopace website",
  "type": "module",
  "private": true,
  "engines": {
    "node": "^18.17.1 || ^20.3.0 || >= 21.0.0"
  },
  "scripts": {
    "astro": "astro",
    "build": "astro build",
    "clean": "rimraf dist/ .astro/",
    "dev": "astro dev",
    "format": "prettier -w .",
    "lint": "eslint .",
    "prepare": "is-ci || husky",
    "preview": "astro preview",
    "start": "astro dev",
    "validate:html": "pnpm build && html-validate dist/"
  },
  "dependencies": {
    "@astrojs/mdx": "4.3.12",
    "@astrojs/partytown": "2.1.4",
    "@astrojs/sitemap": "3.6.0",
    "@astrolib/analytics": "0.6.1",
    "@astrolib/seo": "1.0.0-beta.8",
    "@fontsource-variable/inter": "5.2.8",
    "@iconify-json/flat-color-icons": "1.2.3",
    "@iconify-json/mdi": "1.2.3",
    "@iconify-json/tabler": "1.2.23",
    "@tailwindcss/typography": "0.5.19",
    "@tailwindcss/vite": "^4.1.17",
    "astro": "5.16.0",
    "astro-compress": "2.3.8",
    "astro-embed": "0.9.2",
    "astro-icon": "1.1.5",
    "limax": "4.2.1",
    "lodash.merge": "4.6.2",
    "rehype-plugin-image-native-lazy-loading": "1.2.0",
    "tailwind-merge": "3.4.0",
    "tailwindcss": "4.1.17",
    "unpic": "4.2.2"
  },
  "devDependencies": {
    "@astrojs/check": "0.9.5",
    "@commitlint/cli": "20.1.0",
    "@commitlint/config-conventional": "20.0.0",
    "@cspell/eslint-plugin": "9.3.2",
    "@eslint/js": "9.39.1",
    "@types/js-yaml": "4.0.9",
    "@types/lodash.merge": "4.6.9",
    "@typescript-eslint/eslint-plugin": "8.47.0",
    "@typescript-eslint/parser": "8.47.0",
    "astro-eslint-parser": "1.2.2",
    "eslint": "9.39.1",
    "eslint-plugin-astro": "1.5.0",
    "globals": "16.5.0",
    "html-validate": "10.4.0",
    "husky": "9.1.7",
    "is-ci": "4.1.0",
    "js-yaml": "4.1.1",
    "lint-staged": "16.2.7",
    "prettier": "3.6.2",
    "prettier-plugin-astro": "0.14.1",
    "prettier-plugin-tailwindcss": "^0.7.1",
    "rimraf": "6.1.2",
    "typescript": "5.9.3",
    "typescript-eslint": "8.47.0"
  },
  "lint-staged": {
    "*.{js,ts,astro,vue,jsx,tsx,mjs,cjs}": [
      "eslint",
      "prettier --write"
    ],
    "*.{html,css,md,mdx,json}": "npx prettier --write"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  }
}
